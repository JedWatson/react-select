{
  "name": "react-select",
  "version": "0.4.6",
  "description": "A Select control built with and for ReactJS",
  "main": "lib/Select.js",
  "author": "Jed Watson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JedWatson/react-select.git"
  },
  "dependencies": {
    "classnames": "^1.2.0",
    "lodash": "^3.7.0",
    "react-input-autosize": "^0.4.3"
  },
  "devDependencies": {
    "babel-eslint": "^3.0.1",
    "babel-jest": "^5.0.1",
    "eslint": "^0.19.0",
    "eslint-plugin-react": "^2.1.1",
    "gulp": "^3.8.11",
    "jest-cli": "^0.4.0",
    "react": ">=0.12.0",
    "react-component-gulp-tasks": "^0.4.6"
  },
  "peerDependencies": {
    "react": ">=0.12.0"
  },
  "browserify-shim": {
